def draw_star():
    seth(288)
    begin_fill()
    for i in range(5):
        forward(size) #Note: The size variable is used to control the star's size
        right(180-36)
    end_fill()

bgcolor("blue") #Replace with a hex code from your chosen color palette
penup()
speed(0)
color("yellow")
size = 400
goto(0,200)
draw_star()
color("red")
size=300
goto(0,150)
draw_star()
color("orange")
size=200
goto(0,100)
draw_star()
